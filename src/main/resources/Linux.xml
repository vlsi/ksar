            </Stat>
            <Stat name="miscip2">
                <headerstr>irec6/s fwddgm6/s idel6/s orq6/s asmrq6/s asmok6/s imcpck6/s omcpck6/s fragok6/s fragcr6/s</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip3">
                <headerstr>ihdrerr/s iadrerr/s iukwnpr/s idisc/s odisc/s onort/s asmf/s fragf/s</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip4">
                <headerstr>imsg/s omsg/s iech/s iechr/s oech/s oechr/s itm/s itmr/s otm/s otmr/s iadrmk/s iadrmkr/s  oadrmk/s oadrmkr/s</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip5">
                <headerstr>ierr/s oerr/s idstunr/s odstunr/s itmex/s otmex/s iparmpb/s oparmpb/s isrcq/s osrcq/s iredir/s oredir/s</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip6">
                <headerstr>active/s passive/s iseg/s oseg/s</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip7">
                <headerstr>atmptf/s estres/s retrans/s isegerr/s orsts/s</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip8">
                <headerstr>idgm/s odgm/s noport/s idgmerr/s</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip9">
                <headerstr>active/s passive/s iseg/s oseg/s</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip10">
                <headerstr>imsg6/s omsg6/s iech6/s iechr6/s oechr6/s igmbq6/s  igmbr6/s ogmbr6/s igmbrd6/s ogmbrd6/s irtsol6/s ortsol6/s irtad6/s inbsol6/s onbsol6/s inbad6/s onbad6/s</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip11">
                <headerstr>atmptf/s estres/s retrans/s isegerr/s orsts/s</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip12">
                <headerstr>tcp6sck udp6sck raw6sck ip6-frag</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip13">
                <headerstr>irec6/s fwddgm6/s idel6/s orq6/s asmrq6/s asmok6/s imcpck6/s omcpck6/s fragok6/s fragcr6/s</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="miscip14">
                <headerstr>ihdrer6/s iadrer6/s iukwnp6/s i2big6/s idisc6/s odisc6/s inort6/s onort6/s asmf6/s fragf6/s itrpck6/s</headerstr>
