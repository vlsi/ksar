<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ConfiG PUBLIC "-//NET/ATOMIQUE/KSAR/CONFIG.DTD" "Config.dtd">
<!--
    Description:
        Configure statistics and their corresponding graphs (plots+stacks)
-->

<ConfiG>
