            <Stat name="miscip16">
                <headerstr> CPU MHz</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="cpu7">
                <headerstr>CPU %usr %nice %sys %iowait %steal %irq %soft %guest %gnice %idle</headerstr>
                <graphname>CPU</graphname>
            </Stat>
            <Stat name="kmem5">
                <headerstr>kbmemfree kbmemused %memused kbbuffers kbcached kbcommit %commit kbactive kbinact kbdirty</headerstr>
                <graphname>KMEM2</graphname>
            </Stat>
            <Stat name="hpage">
                <headerstr>kbhugfree kbhugused %hugused</headerstr>
                <graphname>HPAGE</graphname>
            </Stat>

            <!-- GRAPHS -->
            <Graph name="PROC" Title="Proc" type="unique">
                <Plot Title="proc">
                    <cols>proc/s</cols>
                </Plot>
            </Graph>
            <Graph name="CSWCH" Title="Context" type="unique">
                <Plot Title="cwsch">
                    <cols>cswch/s</cols>
                </Plot>
            </Graph>

            <Graph name="INTR" Title="Interrupts" type="multiple">
                <Plot Title="intr">
                    <cols>intr/s</cols>
                </Plot>
            </Graph>
            <Graph name="SWAP" Title="Swap" type="unique">
                <Plot Title="swap">
                    <cols>pswpin/s pswpout/s</cols>
                </Plot>
            </Graph>
            <Graph name="INET1" Title="Interfaces traffic" type="multiple">
                <Plot Title="packet">
                    <cols>rxpck/s txpck/s</cols>
                </Plot>
                <Plot Title="bytes">
                    <cols>rxbyt/s txbyt/s rxkB/s txkB/s</cols>
                </Plot>
                <Plot Title="cast">
                    <cols>rxcmp/s txcmp/s rxmcst/s</cols>
                </Plot>
                <Plot Title="util">
                    <cols>%ifutil</cols>
                </Plot>
            </Graph>
            <Graph name="INET2" Title="Interfaces error" type="multiple">
                <Plot Title="Rx">
                    <cols>rxerr/s rxdrop/s rxfram/s rxfifo/s</cols>
                </Plot>
                <Plot Title="Tx">
                    <cols>txerr/s txdrop/s txcarr/s txfifo/s</cols>
                </Plot>
                <Plot Title="Collision">
                    <cols>coll/s</cols>
                </Plot>
            </Graph>
            <Graph name="PAGING" Title="Paging" type="unique">
                <Plot Title="pgp">
                    <cols>pgpgin/s pgpgout/s</cols>
                </Plot>
                <Plot Title="pg">
                    <cols>activepg inadtypg inaclnpg inatarpg</cols>
                </Plot>
                <Plot Title="fault">
                    <cols>fault/s majflt/s</cols>
                </Plot>
                <Plot Title="free">
                    <cols>pgfree/s pgscank/s pgscand/s pgsteal/s</cols>
                </Plot>
                <Plot Title="vmeff">
                    <cols>vmeff</cols>
                </Plot>
            </Graph>
            <Graph name="DEV_OLD" Title="Devices" type="multiple">
                <Plot Title="tps">
                    <cols>tps</cols>
                </Plot>
                <Plot Title="blks/s">
                    <cols>blks/s</cols>
                </Plot>
            </Graph>
            <Graph name="DEV" Title="Devices" type="multiple">
                <Plot Title="tps">
                    <cols>tps</cols>
                </Plot>
                <Plot Title="sector">
                    <cols>rd_sec/s wr_sec/s</cols>
                </Plot>
                <Plot Title="queue">
                    <cols>avgrq-sz avgqu-sz</cols>
                </Plot>
                <Plot Title="Response [ms]">
                    <cols>await</cols>
                </Plot>
            </Graph>
            <Graph name="IO" Title="I/O" type="unique">
                <Plot Title="tps">
                    <cols>tps rtps wtps</cols>
                </Plot>
                <Plot Title="blocks/s">
                    <cols>bread/s bwrtn/s</cols>
                </Plot>
            </Graph>
            <Graph name="PAGE" Title="Page" type="unique">
                <Plot Title="Page">
                    <cols>frmpg/s shmpg/s bufpg/s campg/s</cols>
                </Plot>
            </Graph>
            <Graph name="KMEM" Title="KMem" type="unique">
                <Plot Title="KMem">
                    <cols>kbmemfree kbmemused kbmemshrd kbbuffers kbcached kbcommit</cols>
                    <format base="1024" factor="1024"></format>
                </Plot>
                <Plot Title="Swap">
                    <cols>kbswpfree kbswpused kbswpcad</cols>
                    <format base="1024" factor="1024"></format>
                </Plot>
                <Plot Title="%">
                    <cols>%memused %swpused %commit</cols>
                </Plot>
            </Graph>
            <Graph name="KSWAP" Title="KSwap" type="unique">
                <Plot Title="Swap">
                    <cols>kbswpfree kbswpused kbswpcad</cols>
                    <format base="1024" factor="1024"></format>
                </Plot>
                <Plot Title="%">
                    <cols>%swpused %swpcad</cols>
                </Plot>
            </Graph>
            <Graph name="LOAD" Title="Load" type="unique">
                <Plot Title="RunQ">
                    <cols>runq-sz</cols>
                </Plot>
                <Plot Title="Plist">
                    <cols>plist-sz</cols>
                </Plot>
                <Plot Title="LA">
                    <cols>ldavg-1 ldavg-5 ldavg-15</cols>
                </Plot>
            </Graph>
            <Graph name="LOAD_9.1.7" Title="Load" type="unique">
                <Plot Title="RunQ">
                    <cols>runq-sz</cols>
                </Plot>
                <Plot Title="Plist">
                    <cols>plist-sz</cols>
                </Plot>
                <Plot Title="Load Average">
                    <cols>ldavg-1 ldavg-5 ldavg-15</cols>
                </Plot>
                <Plot Title="Tasks blocked">
                    <cols>blocked</cols>
                </Plot>
            </Graph>
            <Graph name="SOCKET" Title="Socket" type="unique">
                <Plot Title="Socket">
                    <cols>totsck tcpsck udpsck rawsck</cols>
                </Plot>
                <Plot Title="Frag">
                    <cols>ip-frag tcp-tw</cols>
                </Plot>
            </Graph>
            <Graph name="NFSC" Title="NFS Client" type="unique">
                <Plot Title="call">
                    <cols>call/s retrans/s</cols>
                </Plot>
                <Plot Title="I/O">
                    <cols>read/s write/s</cols>
                </Plot>
                <Plot Title="access">
                    <cols>access/s getatt/s</cols>
                </Plot>
            </Graph>
            <Graph name="NFSM" Title="NFS Server" type="unique">
                <Plot Title="call">
                    <cols>call/s badcalls/s</cols>
                </Plot>
                <Plot Title="I/O">
                    <cols>packet/s udp/s tcp/s</cols>
                </Plot>
                <Plot Title="cache">
                    <cols>hit/s miss/s</cols>
                </Plot>
                <Plot Title="traffic">
                    <cols>sread/s swrite/s saccess/s sgetatt/s</cols>
                </Plot>
            </Graph>
            <Graph name="PROC2" Title="Processes" type="unique">
                <Plot Title="Proc">
                    <cols>proc/s</cols>
                </Plot>
                <Plot Title="context">
                    <cols>cswch/s</cols>
                </Plot>
            </Graph>
            <Graph name="CPU" Title="CPU" type="multiple">
                <Stack Title="Used" size="3">
                    <cols>%usr %user %nice %sys %iowait %steal %irq %soft %guest</cols>
                </Stack>
                <!--
                <Plot Title="other" size="3">
                    <cols>%usr %user %nice %sys %iowait %steal %irq %soft %guest</cols>
                </Plot>
                -->
                <Plot Title="Idle" size="1">
                    <cols>%idle</cols>
                </Plot>
            </Graph>
            <Graph name="KMEM2" Title="KMem" type="unique">
                <Plot Title="KMem">
                    <cols>kbmemfree kbmemused kbbuffers kbcached kbcommit kbactive kbinact kbdirty</cols>
                    <format base="1024" factor="1024"></format>
                </Plot>
                <Plot Title="%">
                    <cols>%memused %commit</cols>
                </Plot>
            </Graph>
            <Graph name="HPAGE" Title="Hugepages" type="unique">
                <Plot Title="Hugepages">
                    <cols>kbhugfree kbhugused</cols>
                    <format base="1024" factor="1024"></format>
                </Plot>
                <Plot Title="%">
                    <cols>%hugused</cols>
                </Plot>
            </Graph>
        </OSType>
    </OS>
    <!--  END OS -->
</ConfiG>

