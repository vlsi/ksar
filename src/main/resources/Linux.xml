            </Stat>
            <Stat name="cpu5">
                <headerstr>CPU %usr %nice %sys %iowait %steal %irq %soft %guest %idle</headerstr>
                <graphname>CPU</graphname>
            </Stat>
            <Stat name="kmem3">
                <headerstr>kbmemfree kbmemused %memused kbmemshrd kbbuffers kbcached kbswpfree kbswpused %swpused</headerstr>
                <graphname>KMEM</graphname>
            </Stat>
            <Stat name="kmem4">
                <headerstr>kbmemfree kbmemused %memused kbbuffers kbcached kbswpfree kbswpused %swpused kbswpcad</headerstr>
                <graphname>KMEM</graphname>
            </Stat>
            <Stat name="dev4">
                <headerstr>DEV tps rd_sec/s wr_sec/s avgrq-sz avgqu-sz await svctm %util</headerstr>
                <graphname>DEV</graphname>
            </Stat>
            <Stat name="dentunusd2">
                <headerstr>dentunusd file-sz inode-sz super-sz %super-sz dquot-sz %dquot-sz rtsig-sz %rtsig-sz</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="paging3">
                <headerstr>pgpgin/s pgpgout/s fault/s majflt/s pgfree/s pgscank/s pgscand/s pgsteal/s %vmeff</headerstr>
                <graphname>PAGING</graphname>
            </Stat>
            <Stat name="inet21">
                <headerstr>IFACE rxerr/s txerr/s coll/s rxdrop/s txdrop/s txcarr/s rxfram/s rxfifo/s txfifo/s</headerstr>
                <graphname>INET2</graphname>
            </Stat>
            <Stat name="dentunusd3">
                <headerstr>dentunusd file-sz %file-sz inode-sz super-sz %super-sz dquot-sz %dquot-sz rtsig-sz %rtsig-sz</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="cpu6">
                <headerstr>CPU %usr %nice %sys %iowait %steal %irq %soft %guest %idle</headerstr>
                <graphname>CPU</graphname>
            </Stat>
            <Stat name="nsfm">
                <headerstr>scall/s badcall/s packet/s udp/s tcp/s hit/s miss/s sread/s swrite/s saccess/s sgetatt/s</headerstr>
                <graphname>NFSM</graphname>
            </Stat>
            <Stat name="miscip1">
                <headerstr>irec/s fwddgm/s idel/s orq/s asmrq/s asmok/s fragok/s fragcrt/s</headerstr>
